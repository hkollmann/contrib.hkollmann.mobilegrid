{
  "environment": {
    "qx.icontheme": "Tango",
    "excludeFromAPIViewer": [
      "qx.*"
    ]
  },
  "applications": [
    {
      "class": "gh.hkollmann.mobilegrid.demo.Application",
      "name": "mobilegrid.demo",
      "title": "MobileGrid Demo"
    }
  ],
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "build",
      "outputPath": "build-output"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {}
  }
}