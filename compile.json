{
	"$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
	"environment": {
		"qx.icontheme": "Tango",
		"excludeFromAPIViewer": [
			"qx.*",
			"qxl.*"
		]
	},
	"applications": [
		{
			"class": "hkollmann.mobilegrid.demo.Application",
			"name": "mobilegrid.demo",
			"title": "MobileGrid Demo",
			"default": true
		},
		{
			"class": "qxl.apiviewer.Application",
			"theme": "qxl.apiviewer.Theme",
			"name": "apiviewer",
			"title": "MobileGrid API",
			"environment": {
				"qx.icontheme": "Tango",
				"excludeFromAPIViewer": [
					"qxl.*",
					"qx.*",
					"qxWeb"
				]
			}
		}
	],
	"targets": [
		{
			"type": "source",
			"outputPath": "compiled/source"
		},
		{
			"type": "build",
			"outputPath": "compiled/build"
		}
	],
	"defaultTarget": "source",
	"locales": [
		"en"
	],
	"eslintConfig": {
		"parserOptions": {
			"ecmaVersion": 2017
		},
		"rules": {}
	},
	"sass": {
	   "compiler": "legacy"
	}
}